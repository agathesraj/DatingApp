{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Ocelot": "Information"
    }
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "SecretKey": "super secret unguessable key",
    "Issuer": "DatingApp",
    "Audience": "DatingAppUsers",
    "ExpirationHours": 24
  },
  "RateLimit": {
    "MaxRequests": 100,
    "WindowInMinutes": 1
  },
  "HealthChecks": {
    "UI": {
      "MaximumExecutionTimePerCheck": "00:00:30"
    }
  },
  "ApiEndpoints": {
    "DatingAPI": "https://localhost:5001"
  }
}
